<span class="n">mall</span><span class="p">.</span><span class="nf">stores</span>
<span class="c1"># =&gt; [#&lt;Store...&gt;]</span>
<span class="n">mall</span><span class="p">.</span><span class="nf">stores</span><span class="p">.</span><span class="nf">count</span>
<span class="c1"># =&gt; 1</span>
<span class="n">mall</span><span class="p">.</span><span class="nf">stores</span><span class="p">.</span><span class="nf">first</span>
<span class="c1"># =&gt; #&lt;Store...&gt;</span>
<span class="n">mall</span><span class="p">.</span><span class="nf">stores</span><span class="p">.</span><span class="nf">first</span><span class="p">.</span><span class="nf">employees</span>
<span class="c1"># =&gt; []</span>

<span class="n">employee_1</span> <span class="o">=</span> <span class="no">Employee</span><span class="p">.</span><span class="nf">new</span><span class="p">(</span><span class="s2">"Rachel"</span><span class="p">,</span> <span class="mi">92836</span><span class="p">,</span> <span class="s2">"$32000"</span><span class="p">)</span>
<span class="n">employee_2</span> <span class="o">=</span> <span class="no">Employee</span><span class="p">.</span><span class="nf">new</span><span class="p">(</span><span class="s2">"Sid"</span><span class="p">,</span> <span class="mi">17638</span><span class="p">,</span> <span class="s2">"$30000"</span><span class="p">)</span>

<span class="n">mall</span><span class="p">.</span><span class="nf">stores</span><span class="p">.</span><span class="nf">first</span><span class="p">.</span><span class="nf">hire_employee</span><span class="p">(</span><span class="n">employee_1</span><span class="p">)</span>
<span class="n">mall</span><span class="p">.</span><span class="nf">stores</span><span class="p">.</span><span class="nf">first</span><span class="p">.</span><span class="nf">hire_employee</span><span class="p">(</span><span class="n">employee_2</span><span class="p">)</span>

<span class="n">mall</span><span class="p">.</span><span class="nf">stores</span><span class="p">.</span><span class="nf">first</span><span class="p">.</span><span class="nf">employees</span>
<span class="c1"># =&gt; [#&lt;Employee...&gt;, #&lt;Employee...&gt;]</span>
<span class="n">mall</span><span class="p">.</span><span class="nf">stores</span><span class="p">.</span><span class="nf">first</span><span class="p">.</span><span class="nf">employees</span><span class="p">.</span><span class="nf">count</span>
